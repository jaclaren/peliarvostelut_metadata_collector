"line":50,"column":0},"generated":{"line":61,"column":14}},{"source":"scraper.ts","name":null,"original":{"line":50,"column":17},"generated":{"line":61,"column":17}},{"source":"scraper.ts","name":null,"original":{"line":50,"column":0},"generated":{"line":61,"column":24}}],"sources":{"scraper.ts":"import { JsxEmit } from 'typescript'\r\nimport { puppeteer } from 'puppeteer'\r\nimport { mysqlCrawl } from './mysql'\r\nimport mysql from 'mysql'\r\n\r\nclass Scraper {  \r\n  async crawl_html(url) { \r\n    const browser = await puppeteer.launch({ headless : true })\r\n    const page = await browser.newPage()\r\n    await page.goto(url)\r\n    await page.screenshot({path: 'example.png'})\r\n  \r\n    const data = await page.evaluate(() => document.querySelector('*').outerHTML)\r\n    await browser.close()  \r\n    return data\r\n  }\r\n\r\n  async save_html_to_database(url, data, mysqlCrawl) {\r\n    const sql = mysql.format(`INSERT INTO scraped_html (url, html) VALUES ?`)\r\n    return mysqlCrawl.insertQuery(sql, [mysql.escape(url), mysql.escape(data)])\r\n  }\r\n  \r\n  async get_html_from_url(url, mysqlCrawl) {\r\n    const sql = `SELECT url, html FROM scraped_html WHERE url = '${url}'`\r\n\r\n    return new Promise((resolve, reject) => {\r\n      mysqlCrawl.query(sql, async (err, results) => {\r\n        if(err)\r\n          reject(err)\r\n        if(results.length == 0) {\r\n          const html = await this.crawl_html(url)     \r\n          \r\n          this.save_html_to_database(url, html, mysqlCrawl)\r\n            .then(_res => resolve({\r\n              existedInDatabase : false, \r\n              html\r\n            }))\r\n          \r\n        } else {\r\n          return resolve({\r\n            existedInDatabase : true,\r\n            html : results[0].html\r\n          })\r\n        }\r\n      })\r\n    })\r\n  }\r\n}\r\n\r\nmodule.exports = Scraper"},"lineCount":null}},"error":null,"hash":"bea8087628001a2ed03522af4889f17b","cacheData":{"env":{}}}